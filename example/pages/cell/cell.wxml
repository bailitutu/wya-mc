
<view>
    <notes>最简单的 cell</notes>
	<cell title="我是标题" bind:tap="handleTap"></cell>
</view>

<view>
    <notes>带详细信息的 cell</notes>
	<cell title="我是标题" value="详细信息"></cell>
</view>

<view>
    <notes>带附加描述的 cell</notes>
	<cell title="我是标题" label="附加描述" value="详细信息"></cell>
</view>

<view>
    <notes>带左边 icon 的 cell</notes>
    <cell title="我是标题" value="详细信息" label="附加描述">
      <icon type="success" classes="g-m-r-10"/>
    </cell>
</view>

<view>
    <notes>带右边 icon 的 cell</notes>
    <cell title="我是标题" value="详细信息">
        <icon type="success"/>
    </cell>
</view>

<view>
    <notes>只显示箭头</notes>
    <cell 
		title="我是标题" 
		value="详细信息"
		is-link
	></cell>
</view>

<view>
    <notes>跳转到其他页面</notes>
    <cell 
		title="我是标题" 
		value="详细信息"
		is-link
		url="/pages/dashboard/index"
	></cell>
</view>
